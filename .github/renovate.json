{
  "dependencyDashboard": true,
  "enabledManagers": ["gradle"],
  "packageRules": [
    {
      "matchDatasources": ["maven"],
      "matchPackageNames": [
        "/androidx\\.compose\\..*/",
        "/androidx\\.media3\\..*/"
      ],
      "rangeStrategy": "bump",
      "versioning": "loose"
    }
  ],
  "labels": ["dependencies"],
  "automerge": false,
  "rebaseWhen": "behind-base-branch"
}
